<!DOCTYPE html>
<html>
<head>
  <title>Cmngsn</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% [:notice, :error, :alert].each do |level| %>
	  <% unless flash[level].blank? %>
	    <div class="<%= level %>">
	      <%= content_tag :p, flash[level] %>
	    </div>
	  <% end %>
	<% end %>
	<% flash.discard %>

<%= yield %>

</body>
</html>
